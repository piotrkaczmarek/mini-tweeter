<% provide(:title, "Edit organization") %>
<h1>Update your organization profile</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@organization) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :homesite_url %>
      <%= f.text_field :homesite_url %>
      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>

  </div>
</div>
